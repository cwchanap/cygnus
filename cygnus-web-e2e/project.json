{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "cygnus-web-e2e",
  "root": "cygnus-web-e2e",
  "projectType": "application",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test",
        "cwd": "{projectRoot}"
      }
    },
    "test-ui": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run test:ui",
        "cwd": "{projectRoot}"
      },
      "outputs": []
    }
  },
  "implicitDependencies": ["cygnus-web"],
  "tags": ["scope:web", "type:e2e"]
}
